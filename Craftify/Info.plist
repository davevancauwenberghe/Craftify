<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleSupportedPlatforms</key>
    <array>
        <string>iPhoneOS</string>
    </array>
    <key>MinimumOSVersion</key>
    <string>17.0</string>
    <key>NSMailComposeUsageDescription</key>
    <string>Craftify uses email to let you report missing recipes and contact support.</string>
    <key>NSUbiquitousContainers</key>
    <dict>
        <key>iCloud.be.ghentphotography.www.Craftify</key>
        <dict>
            <key>NSUbiquitousContainerIsDocumentScopePublic</key>
            <true/>
            <key>NSUbiquitousContainerName</key>
            <string>Craftify</string>
            <key>NSUbiquitousContainerSupportedFolderLevels</key>
            <string>All</string>
        </dict>
    </dict>
    <key>CFBundleIcons</key>
    <dict>
        <!-- Primary (default) icon asset set name -->
        <key>CFBundlePrimaryIcon</key>
        <dict>
            <key>CFBundleIconName</key>
            <string>AppIcon</string>
            <key>UIPrerenderedIcon</key>
            <false/>
        </dict>
        <key>CFBundleAlternateIcons</key>
        <dict>
            <key>AlternateIcon1</key>
            <dict>
                <key>CFBundleIconName</key>
                <string>AlternateIcon1</string>
                <key>UIPrerenderedIcon</key>
                <false/>
            </dict>
            <key>AlternateIcon2</key>
            <dict>
                <key>CFBundleIconName</key>
                <string>AlternateIcon2</string>
                <key>UIPrerenderedIcon</key>
                <false/>
            </dict>
        </dict>
    </dict>
</dict>
</plist>
